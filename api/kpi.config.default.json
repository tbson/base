{
    "ASSIGNMENT_STATUS": [40, 60, 90],

    "QUOTA_RESULT_NORMAL_FORMULA": {
        "rule": "weight*(X+(1-X)*(achieve-minimum)/(expected-minimum))",
        "cond": [["expected - minimum != 0", "Tối thiểu không thể bằng kỳ vọng"]]
    },
    "QUOTA_RESULT_INVERSE_FORMULA": {
        "rule": "weight*(X+(1-X)*(minimum-achieve)/(minimum-expected))",
        "cond": [["minimum - expected != 0", "Tối thiểu không thể bằng kỳ vọng"]]
    },

    "QUOTA_EXCEED_NORMAL_FORMULA": {
        "rule": "weight*(achieve/expected-1)",
        "cond": [["expected != 0", "Kì vọng không thể = 0"]]
    },
    "QUOTA_EXCEED_INVERSE_FORMULA": {
        "rule": "weight*(1-achieve/expected)",
        "cond": [["expected != 0", "Kì vọng không thể = 0"]]
    },

    "QUOTA_MINIMUM_BONUS_THRESHOLD": 80,
    "QUOTA_MAXIMUM_EXCEED_WEIGHT": 100
}
